        "hw.cpp",
        "jdsim.ts",
        "hw.h",
        "mbbridge.h",
        "mbbridge.cpp",
        "JacdacBLE.h",
        "JacdacBLE.cpp",
        "jdble.cpp",
        "mbcompat.ts",
        "commands.ts",
        "config.ts",
        "packet.ts",
        "pack.ts",
        "service.ts",
        "pipes.ts",
        "actuator.ts",
        "diagnostics.ts",
        "modules.ts",
        "eventqueue.ts",
        "ns.ts",
        "system/constants.ts",
        "base/constants.ts",
        "bootloader/constants.ts",
        "control/constants.ts",
        "logger/constants.ts",
        "logger/client.ts",
        "logger-server/server.ts",
        "settings/constants.ts",
        "settings/client.ts",
        "settings-server/server.ts",
        "sensor/constants.ts",
        "sensor/client.ts",
        "sensor/server.ts",
        "role-manager/constants.ts",
        "rolemgr.ts",
        "proto-test/constants.ts",
        "proto-test/server.ts",
        "routing.ts"
