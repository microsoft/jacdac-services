    },
    "supportedTargets": [
        "arcade",
        "maker",
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "disablesVariants": [
        "mbdal"
    ],
    "binaryonly": true,
    "extension": {
        "namespace": "modules",
        "label": "Configure",
        "group": "Services",
        "url": "https://microsoft.github.io/jacdac-docs/tools/makecode-editor-extension",
        "localUrl": "http://localhost:8000/tools/makecode-editor-extension"
    }
